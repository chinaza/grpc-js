{
  "name": "grpc-rest",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/module.js",
  "source": "src/index.ts",
  "scripts": {
    "build": "parcel build",
    "build:dev": "tsc --watch",
    "format": "prettier --write .",
    "lint": "eslint . --fix",
    "test": "jest --passWithNoTests",
    "test-watch": "jest --watch --passWithNoTests",
    "start": "node dist/index.js",
    "start:dev": "nodemon --exec ts-node --transpile-only ./src/index.ts",
    "prepare": "npm run build"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "dist"
  ],
  "engineStrict": true,
  "author": {
    "name": "naza",
    "url": "https://chinaza.dev"
  },
  "alias": {
    "@src/*": "./src/$1"
  },
  "prettier": "@chinaza/eslint-config/prettier",
  "eslintConfig": {
    "extends": [
      "@chinaza/eslint-config/express"
    ]
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.6.7",
    "@grpc/proto-loader": "^0.6.13",
    "module-alias": "^2.2.2"
  },
  "devDependencies": {
    "@chinaza/eslint-config": "^1.0.5",
    "@parcel/compressor-brotli": "^2.5.0",
    "@parcel/compressor-gzip": "^2.5.0",
    "@parcel/packager-ts": "^2.5.0",
    "@parcel/transformer-typescript-types": "^2.5.0",
    "@types/jest": "^27.4.0",
    "@types/module-alias": "^2.0.1",
    "@types/node": "^17.0.7",
    "eslint": "^8.6.0",
    "jest": "^27.4.5",
    "nodemon": "^2.0.15",
    "parcel": "^2.6.2",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  }
}
